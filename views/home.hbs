 <div class="modal  fade" id="exampleModalLong" tabindex="-1" role="dialog" aria-labelledby="exampleModalLongTitle" aria-hidden="true">
    <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLongTitle"></h5>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
                <div class="modal-body">
                <!--/ Form Search Star /-->
                        <div class="title-box-d">
                        <h3 class="title-d">Search Product/Service</h3>
                        </div>
                        <div class="form">
                        <form class="form-a" action="/searchResults" method="post">
                            <div class="row">
                            <div class="col-md-12 mb-2">
                                <div class="form-group">
                                <label class="pb-2" for="name">Name</label>
                                <input type="text" class="form-control form-control-lg form-control-a" placeholder="name" name="name">
                                </div>
                            </div>
                            <div class="col-md-6 mb-2">
                                <div class="form-group mt-3">
                                <label class="pb-2" for="location">Town</label>
                                <select class="form-control form-select form-control-a" id="city">
                                    <option value="limbe">Limbe</option>
                                    <option value="bamenda">Bamenda</option>
                                    <option value="yaounde">Yaounde</option>
                                    <option value="douala">Douala</option>
                                    <option value="buea">Buea</option>
                                    <option value="baffoussam">Baffoussam</option>
                                </select>
                                </div>
                            </div>
                            
                            <div class="col-md-12">
                                <button type="submit" class="btn btn-b">Search</button>
                            </div>
                            </div>
                        </form>
                        </div>
                    <!-- End Property Search Section -->
                 </div>
            </div>
    </div>
</div>
